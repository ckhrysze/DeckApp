<% runs.each do |run| %>
<tr id="run<%= run.id%>">
  <td>&nbsp;</td>
  <td class="run_count"><%= run.count %></td>
  <td class="card_name"><%= run.name %></td>
  <td class="mtg_id"><%= run.mtg_id %></td>
  <td class="cc"><%= run.cc %></td>
  <td class="cmc"><%= run.cmc %></td>
  <td><span class='delete_run' href=''>X</span></td>
</tr>
<% end %>